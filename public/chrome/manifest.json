{
  "manifest_version": 2,
  "name": "Toolbar Dial",
  "version": "2.1",
  "permissions": ["bookmarks", "storage"],
  "description": "Toolbar Dial is a browser extension that replaces the New Tab page with a colorful grid of bookmarks and folders.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "32": "icon-32.png",
    "48": "icon-48.png",
    "96": "icon-96.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  },
  "author": "Lucas Everett",
  "homepage_url": "https://toolbardial.com"
}
